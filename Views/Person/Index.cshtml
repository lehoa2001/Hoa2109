<h2>danh sách cá nhân</h2>
<hr>
<table class="table border">
    <tr>
        <th>
            <label>ma ca nhan</label>
        </th>
        <th>
            <label>ten ca nhan</label>
        </th>
        <th>
            <label>tuoi</label>
        </th>
    </tr>
    <tr>
        @foreach (var item in ViewData["Persons"] as List<Person>)
            {
                <tr>
                    <td>@item.PersonId</td>
                    <td>@item.PersonName</td>
                    <td>@item.PersonAge</td>
                </tr>
            }
    </tr>
</table>